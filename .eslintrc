{
  "extends": [
    "airbnb",
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "no-console": 0,
    "import/newline-after-import": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "no-underscore-dangle": 0,
    "quote-props": 0,
    "prefer-destructuring": 0,
    "no-continue": 0,
    "block-scoped-var": 0,
    "vars-on-top": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 0,
    "no-var": 0,
    "no-param-reassign": 1,
    "max-len": 1,
    "prefer-arrow-callback": 1,
    "prefer-template": 1,
    "consistent-return": 1,
  }
}
